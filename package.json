{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production next build",
    "ci": "bun run format:check && bun run lint && bun run test",
    "clean": "rm -rf -- node_modules/ dist/ .next/ .vercel/",
    "dev": "next dev --turbo",
    "format": "prettier -w . ; sort-package-json",
    "format:check": "prettier --check . && sort-package-json --check && cargo fmt --check",
    "lint": "next lint --max-warnings=0",
    "prepare": "husky",
    "update": "bun x npm-check-updates -ui -ws --root"
  },
  "prettier": {
    "jsxSingleQuote": true,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "singleQuote": true
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "5.66.0",
    "@vercel/analytics": "^1.5.0",
    "ag-grid-react": "33.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "debug": "4.4.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "geist": "^1.5.1",
    "lucide-react": "^0.543.0",
    "next": "15.1.6",
    "react": "19.0.0",
    "react-aria-components": "1.6.0",
    "react-dom": "19.0.0",
    "react-icons": "5.4.0",
    "server-only": "0.0.1",
    "surrealdb": "1.1.0",
    "tailwind-merge": "3.0.1",
    "tw-animate-css": "^1.3.8",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "1.26.0",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.19.0",
    "@mdx-js/loader": "3.1.0",
    "@mdx-js/react": "3.1.0",
    "@next/mdx": "15.1.6",
    "@tailwindcss/typography": "0.5.16",
    "@tanstack/eslint-plugin-query": "5.66.0",
    "@types/bun": "1.2.2",
    "@types/eslint__eslintrc": "2.1.2",
    "@types/mdx": "2.0.13",
    "@types/node": "22.10.10",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "autoprefixer": "^10.4.21",
    "eslint-plugin-react-hooks": "5.1.0",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "postcss": "8.5.1",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "sort-package-json": "2.14.0",
    "tailwindcss": "^3.4.0",
    "turbo": "2.4.0",
    "typescript": "5.7.3",
    "typescript-eslint": "8.22.0",
    "vercel": "40.1.0"
  },
  "packageManager": "bun@1.2.2"
}
